{
    "name": "jandrda/olapus",
    "description": "Simple out of box solution for easy administering of any website.",
    "keywords": ["laravel", "cms"],
    "license": "MIT",
    "type": "project",
    "homepage": "https://www.olapus.com/",
    "authors": [
        {
            "name": "Jan Drda",
            "email": "jdrda@outlook.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "support@olapus.com"
    },
    "require": {
        "php": ">=7.0.0",
        "fideloper/proxy": "~3.3",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "greggilbert/recaptcha": ">=2.1.1",
        "spatie/laravel-analytics": "1.4.*",
        "doctrine/dbal": ">=2.5.4",
        "laravolt/avatar": ">=1.8.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "symfony/dom-crawler": "3.1.*",
        "symfony/css-selector": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
      "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
